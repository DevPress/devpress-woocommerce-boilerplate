{
    "name": "devpress/devpress-woocommerce-boilerplate",
    "description": "A foundation for building high-quality WooCommerce plugins.",
    "keywords": [
        "wordpress"
    ],
    "type": "wordpress-plugin",
    "homepage": "https://github.com/devpress/devpress-woocommerce-boilerplate",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Devin Price",
            "homepage": "https://devpress.com"
        }
    ],
    "support": {
        "issues": "https://github.com/devpress/devpress-woocommerce-boilerplate/issues",
        "source": "https://github.com/devpress/devpress-woocommerce-boilerplate"
    },
    "require-dev": {
        "automattic/vipwpcs": "^2.3",
        "assertwell/wp-core-test-framework": "^0.1.1",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "php": ">=7.4",
        "phpunit/phpunit": "^7",
        "yoast/phpunit-polyfills": "^1.0",
        "wp-cli/i18n-command": "^2.2.5"
    },
    "scripts": {
        "make-pot": "wp i18n make-pot . languages/devpress-woocommerce-boilerplate.pot"
    },
    "autoload-dev": {
        "psr-4": {
            "DevPress_WooCommerce_Boilerplate\\Test\\": "tests/phpunit/"
        },
        "classmap": [
            "includes/"
        ]
    }
}
